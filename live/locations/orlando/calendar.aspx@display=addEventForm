        if ((window.location.hash.indexOf('members.hardrock') !== -1)) {
            if (window.location.hash.indexOf('email=') !== -1) {
                emailQuery = decodeURIComponent($.trim(window.location.hash.split('=')[1] || ''));

            } else {
                emailQuery = decodeURIComponent($.trim(window.location.search.split('=')[1] || ''));

            }
            if (emailQuery) {

                $('#MainContent_RegistrationFree_txtEmailAddress').val(query);
            }
